    "start:extraTs": "tsc-watch --project . --onSuccess \"npm run nodemon\"",
    "nodemon": "nodemon ./dist/index.js",
    "lint": "./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json --exclude 'node_modules/**/*.{j,t}s'",
    "lint:fix": "./node_modules/tslint/bin/tslint --fix -c tslint.json -p tsconfig.json --exclude 'node_modules/**/*.{j,t}s'" ,
    "prepare": "husky install",
    "scriptname": "cmd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amrendra-rathore/express-server.git"
  },
  "author": "",
