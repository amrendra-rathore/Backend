{
  "name": "express-server",
  "version": "1.0.0",
  "description": "Learing Backend - Node.js",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "nodemon src/index.ts",
    "build": "npx babel-node extra",
    "start:extra": "babel-node extra",
    "start:extraTs": "tsc-watch --project . --onSuccess \"npm run nodemon\"",
    "nodemon": "nodemon ./dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "prepare": "husky install",
    "scriptname": "cmd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amrendra-rathore/express-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amrendra-rathore/express-server/issues"
  },
  "homepage": "https://github.com/amrendra-rathore/express-server#readme",
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@babel/node": "7.18.5",
    "@babel/preset-env": "7.18.2",
    "@types/express": "4.17.13",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "cz-conventional-changelog": "3.0.1",
    "eslint": "8.19.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-no-loops": "0.3.0",
    "husky": "8.0.1",
    "ts-node": "10.8.1",
    "tsc-watch": "5.0.3",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin-tslint": "5.30.5",
    "commitizen": "1.0.4",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-validator": "6.14.2",
    "jsonwebtoken": "8.5.1",
    "mongoose": "^6.4.6",
    "nodemon": "2.0.18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "precommit": "npm run lint"
  }
